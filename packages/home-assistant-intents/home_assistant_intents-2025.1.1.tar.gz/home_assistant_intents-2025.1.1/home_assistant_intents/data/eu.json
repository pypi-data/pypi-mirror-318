      "HassGetState": {
        "default": "{{ state.state_with_unit }}\n",
        "one": "{{ slots.name | capitalize }} gailuaren egoera {{ state.state_with_unit }} da\n",
        "one_yesno": "{% if query.matched %}\n  Bai\n{% else %}\n  Ez, {{ state.state_with_unit }} dago\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    Bai, {{ match[:3] | join(\", \") }} eta beste {{ (match | length - 3) }} gehiago\n  {%- else -%}\n    {% if match | length == 1 %}\n      Bai, {{ match|first }}\n    {% else %}\n      Bai,\n      {% for name in match -%}\n        {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} eta {% endif -%}\n        {{ name }}\n      {%- endfor -%}\n    {% endif %}\n  {% endif %}\n{% else %}\n  Ez\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  Bai\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    Ez, {{ no_match[:3] | join(\", \") }} eta beste {{ (no_match | length - 3) }} ere ez\n  {% else %}\n    {% if no_match | length == 1 %}\n      Ez, {{ no_match|first }} ez\n    {%- else -%}\n      Ez,\n      {% for name in no_match -%}\n        {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} eta {% endif -%}\n        {{ name }}\n      {%- endfor %}\n      ez daude\n    {% endif %}\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  Gailu bat bera ere ez\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3] | join(\", \") }} eta beste {{ (match | length - 3) }} gehiago\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} eta {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },
