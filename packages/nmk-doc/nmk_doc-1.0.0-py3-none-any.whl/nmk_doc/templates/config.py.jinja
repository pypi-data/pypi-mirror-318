# Configuration file for the Sphinx documentation builder.
# Please don't edit: generated by nmk-doc plugin
# Contribute to "docXxx" config items in nmk.yml instead!

# For the full list of built-in configuration values, see the documentation:
# https://www.sphinx-doc.org/en/master/usage/configuration.html

# Project information
project = "{{ docName }}"
copyright = "{{ docYear }}, {{ docAuthor }}"
author = "{{ docAuthor }}"

# Extensions configuration
extensions = [{% for extension in docExtensions %}{% if loop.index > 1 %}, {% endif %}"{{ extension }}"{% endfor %}]

# HTML theme
html_theme = "{{ docHtmlTheme }}"

# Extra configuration
{% for cfg in docExtraConfig %}
{{ cfg }} = {% if docExtraConfig[cfg] is string %}"{{docExtraConfig[cfg]}}"{% else %}{{docExtraConfig[cfg]}}{% endif %}{% endfor %}

