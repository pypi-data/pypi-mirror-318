/* QMenu ///////////////////////////////////////////////////////////////////// */
QMenu {
    background-color: rgb(33, 37, 43);
    border-radius: 10px;
}
/* 可用，且选中的选项 */
QMenu::item:enabled:selected{
    color: rgb(255, 121, 198);
    background-color: rgb(39, 44, 54);
}
/* 不可用选项 */
QMenu::item:disabled {
    color: rgb(113, 126, 149);
}